{
	"name": "chatventure/swyf-sdk",
	"description": "Library for communication with the Swyf API for faceted lookbook and product search.",
	"keywords": ["sdk", "swyf"],
	"homepage": "https://github.com/Chatventure/swyf-sdk",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Jasper van Wanrooy",
			"email": "jvanwanrooy@swyf.com"
		}
	],
	"support": {
		"issues": "https://github.com/Chatventure/swyf-sdk/issues",
		"email": "team@swyf.com"
	},
	"require": {
		"php": ">=5.3.0",
		"ext-json": "*",
		"ext-curl": "*"
	},
	"autoload": {
		"psr-0": {
			"Swyf": "library"
		}
	},
	"include-path": ["library/"],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/Chatventure/swyf-sdk"
		}
	]
}